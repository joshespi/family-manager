{
    "name": "joshespi/family-manager",
    "description": "Web application for managing family tasks",
    "type": "project",
    "require": {
        "vlucas/phpdotenv": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "App\\Models\\": "models/",
            "App\\Controllers\\": "controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Models\\": "src/models/",
            "App\\Controllers\\": "src/controllers/",
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/php-code-coverage": "^11.0",
        "phpunit/phpunit": "^11.5"
        
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
